<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<at.tugraz.ist.catroid.tutorial.LessonCollection>
	<lessonArray>
		<at.tugraz.ist.catroid.tutorial.Lesson>
			<lessonName>Demonstration</lessonName>
			<lessonContent>
				<at.tugraz.ist.catroid.tutorial.tasks.TaskSleep>
					<sleepTime>1000</sleepTime>
				</at.tugraz.ist.catroid.tutorial.tasks.TaskSleep>
				
				<at.tugraz.ist.catroid.tutorial.tasks.TaskFlip>
				    <tutorType>CATRO</tutorType>
				    <flipFast>true</flipFast>
				</at.tugraz.ist.catroid.tutorial.tasks.TaskFlip>
				
 				<at.tugraz.ist.catroid.tutorial.tasks.TaskAppear>
					<tutorType>CATRO</tutorType>
					<x>80</x>
					<y>300</y>
				</at.tugraz.ist.catroid.tutorial.tasks.TaskAppear> 
				
 				<at.tugraz.ist.catroid.tutorial.tasks.TaskSay>
                    <tutorType>CATRO</tutorType>
                    <message>Hallo! Ich bin CATRO- dein Tutor. Ich werde dir in aller Kürze erklären wie du uns Tutoren steuern kannst.</message>
                </at.tugraz.ist.catroid.tutorial.tasks.TaskSay>
                <at.tugraz.ist.catroid.tutorial.tasks.TaskSleep>
                    <tutorType>CATRO</tutorType>
                    <sleepTime>1000</sleepTime>
                </at.tugraz.ist.catroid.tutorial.tasks.TaskSleep>
                
 				<at.tugraz.ist.catroid.tutorial.tasks.TaskSay>
                    <tutorType>CATRO</tutorType>
                    <message>Alle Aktionen kannst du in der tutorial.xml Datei erstellen. Nun lassen wir uns mal den lieben MIAUS kommen... Mit dem Task APPEAR geht das ganz einfach. Schauen wir einmal: TASKAPPEAR</message>
                </at.tugraz.ist.catroid.tutorial.tasks.TaskSay>
                
 				<at.tugraz.ist.catroid.tutorial.tasks.TaskAppear>
 				    <tutorType>MIAUS</tutorType>
 				    <x>200</x>
 				    <y>300</y>
 				</at.tugraz.ist.catroid.tutorial.tasks.TaskAppear>

 				<at.tugraz.ist.catroid.tutorial.tasks.TaskSay>
                    <tutorType>MIAUS</tutorType>
                    <message>NARF!</message>
                </at.tugraz.ist.catroid.tutorial.tasks.TaskSay>
                
 				<at.tugraz.ist.catroid.tutorial.tasks.TaskFlip>
 				    <tutorType>MIAUS</tutorType>
 				    <flipFast>false</flipFast>
 				</at.tugraz.ist.catroid.tutorial.tasks.TaskFlip>
 				
 				<at.tugraz.ist.catroid.tutorial.tasks.TaskWalk>
 				    <tutorType>MIAUS</tutorType>
 				    <walkToX>50</walkToX>
 				    <walkToY>50</walkToY>
 				    <fastWalk>true</fastWalk>
 				</at.tugraz.ist.catroid.tutorial.tasks.TaskWalk>
 				
 				<at.tugraz.ist.catroid.tutorial.tasks.TaskSay>
                    <tutorType>MIAUS</tutorType>
                    <message>Hallo, ich bin MIAUS... ich bin hier um alles zu tun was CATRO mir so sagt!</message>
                </at.tugraz.ist.catroid.tutorial.tasks.TaskSay>
				
			</lessonContent>
			<lessonID>0</lessonID>
			<currentStep>0</currentStep>
		</at.tugraz.ist.catroid.tutorial.Lesson>
		<at.tugraz.ist.catroid.tutorial.Lesson>
			<lessonName>Multithreading</lessonName>
			<lessonContent>
				<at.tugraz.ist.catroid.tutorial.tasks.TaskSleep>
					<sleepTime>500</sleepTime>
				</at.tugraz.ist.catroid.tutorial.tasks.TaskSleep>
				<at.tugraz.ist.catroid.tutorial.tasks.TaskAppear>
					<tutorType>CATRO</tutorType>
					<x>100</x>
					<y>200</y>
				</at.tugraz.ist.catroid.tutorial.tasks.TaskAppear>
				<at.tugraz.ist.catroid.tutorial.tasks.TaskFlip>
					<tutorType>CATRO</tutorType>
				</at.tugraz.ist.catroid.tutorial.tasks.TaskFlip>

				<at.tugraz.ist.catroid.tutorial.tasks.TaskSleep>
					<sleepTime>500</sleepTime>
				</at.tugraz.ist.catroid.tutorial.tasks.TaskSleep>
				<at.tugraz.ist.catroid.tutorial.tasks.TaskAppear>
					<tutorType>MIAUS</tutorType>
					<x>300</x>
					<y>200</y>
				</at.tugraz.ist.catroid.tutorial.tasks.TaskAppear>
				<at.tugraz.ist.catroid.tutorial.tasks.TaskFlip>
					<tutorType>CATRO</tutorType>
				</at.tugraz.ist.catroid.tutorial.tasks.TaskFlip>

				<at.tugraz.ist.catroid.tutorial.tasks.TaskSay>
					<tutorType>CATRO</tutorType>
					<message>Original Schweizer Kräuterbonbons... nom nom nom!
					</message>
				</at.tugraz.ist.catroid.tutorial.tasks.TaskSay>
				<at.tugraz.ist.catroid.tutorial.tasks.TaskSay>
					<tutorType>MIAUS</tutorType>
					<message>Tell me more about it! woof woof woof *scratch*</message>
				</at.tugraz.ist.catroid.tutorial.tasks.TaskSay>

				<at.tugraz.ist.catroid.tutorial.tasks.TaskSleep>
					<tutorType>CATRO</tutorType>
					<sleepTime>1000</sleepTime>
				</at.tugraz.ist.catroid.tutorial.tasks.TaskSleep>
			</lessonContent>
			<lessonID>1</lessonID>
			<currentStep>0</currentStep>
		</at.tugraz.ist.catroid.tutorial.Lesson>
		<at.tugraz.ist.catroid.tutorial.Lesson>
			<lessonName>Daemonenaustreibung mit Catroid</lessonName>
			<lessonContent>
				<at.tugraz.ist.catroid.tutorial.tasks.TaskAppear>
					<tutorType>CATRO</tutorType>
					<x>100</x>
					<y>300</y>
				</at.tugraz.ist.catroid.tutorial.tasks.TaskAppear>
				<at.tugraz.ist.catroid.tutorial.tasks.TaskSay>
					<tutorType>CATRO</tutorType>
					<message>ROAR! WOOOAAAAAA! GRRRRRRRRR .... miau</message>
				</at.tugraz.ist.catroid.tutorial.tasks.TaskSay>
			</lessonContent>
			<lessonID>2</lessonID>
			<currentStep>0</currentStep>
		</at.tugraz.ist.catroid.tutorial.Lesson>
	</lessonArray>
	<currentLesson>0</currentLesson>
</at.tugraz.ist.catroid.tutorial.LessonCollection>
